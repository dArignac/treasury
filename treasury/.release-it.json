{
  "verbose": true,
  "buildCommand": "sed -i 's/Version [0-9]\\.[0-9]\\.[0-9]/Version ${version}/g' src/app/app.component.html",
  "src": {
    "tagName": "v%s"
  },
  "npm": {
    "publish": false
  },
  "github": {
    "release": false
  },
  "dist": {
    "github": {
      "release": false
    }
  },
  "prompt": {
    "src": {
      "status": true
    },
    "dist": {
      "status": true,
      "tag": true,
      "release": true,
      "publish": true
    }
  }
}
